}

.iconFalse {
background-color:#FF1A00;
background-image: url(/dfbs/aepermits/img/clearbits/bg_rounded.gif);
background-repeat: no-repeat;
margin-top: 5px;
}

.icon {
background-color:#000;
background-image: url(/dfbs/aepermits/img/clearbits/bg_rounded.gif);
background-repeat: no-repeat;
}

.icon2 {
  background-color:#505050;
  background-image: url(/dfbs/aepermits/img/clearbits/bg_rounded.gif);
  background-repeat: no-repeat;
}

.icon3 {
  background-color:#a5a5a5;
  background-image: url(/dfbs/aepermits/img/clearbits/bg_rounded.gif);
  background-repeat: no-repeat;
