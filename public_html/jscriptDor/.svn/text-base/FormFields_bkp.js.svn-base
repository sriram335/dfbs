function FormFields(regMonth)
{
 if (document.myForm.COMBINED_GROSS_WEIGHT.value == "") 
  {
    alert("Enter the value for CGW");
    document.myForm.COMBINED_GROSS_WEIGHT.focus();
  }
 else if (document.myForm.VEH_PURCHASE_PRICE.value == "")
	{
		alert("Enter Purchase Price");
		document.myForm.VEH_PURCHASE_PRICE.focus();
	}
 else if (document.myForm.VEH_PURCHASE_YEAR.value =="")
  {
    alert("Please enter PURCHASE DATE");
    document.myForm.VEH_PURCHASE_YEAR.focus();
  }
  else if (document.myForm.INS_POLICY.value =="")
  {
    alert("Please enter INS POLICY");
    document.myForm.INS_POLICY.focus();
  } 
 else if (document.myForm.INS_COMPANY.value =="")
  {
    alert("Please enter INS COMPANY");
    document.myForm.INS_COMPANY.focus();
  }
 else if (document.myForm.VEHICLE_MAKE.value =="")
  {
    alert("Please enter VEHICLE MAKE");
    document.myForm.VEHICLE_MAKE.focus();
  }
 else if (document.myForm.AXLES.value =="")
  {
    alert("Please enter AXLES");
    document.myForm.AXLES.focus();
  }
  else if (document.myForm.UNLADEN_WEIGHT.value == "") 
  {
    alert("Please enter Unladen Weight");
    document.myForm.UNLADEN_WEIGHT.focus();
  }
  else if (document.myForm.GROSS_WEIGHT.value == "") 
  {
    alert("Please enter Gross Weight");
    document.myForm.GROSS_WEIGHT.focus();
  }
  else
	ValidateFields(regMonth);
 }