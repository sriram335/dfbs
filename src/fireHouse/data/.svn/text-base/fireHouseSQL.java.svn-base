package fireHouse.data;

public class fireHouseSQL {
  public final static String UPLOAD_DOCUMENT =
     " insert into DFBS_DOCUMENTS_FIREHOUSE(DOCUMENT_ID,DOCUMENT_NAME,DOCUMENT_TYPE,DOCUMENT_CONTENT, " +
     " DOCUMENT_UPLOAD_DATE,DOCUMENT_UPLOADED_BY,DOCUMENT_CONNECTOR,DOCUMENT_CONNECTOR_TYPE,DOCUMENT_STATUS)" +
     " values(?,?,?,?,SYSDATE,?,?,?,'Pending')";
      public final static String SELECT_DOCUMENT =
     " SELECT DOCUMENT_CONTENT,DOCUMENT_TYPE,DOCUMENT_ID from DFBS_DOCUMENTS_FIREHOUSE where DOCUMENT_ID=?";
     public final static String SELECT_DOCUMENT_LIST =
     " SELECT DOCUMENT_NAME , DOCUMENT_TYPE, DOCUMENT_UPLOAD_DATE,DOCUMENT_ID ,DOCUMENT_UPLOADED_BY,DOCUMENT_STATUS" +
     " from DFBS_DOCUMENTS_FIREHOUSE WHERE DOCUMENT_CONNECTOR=? AND DOCUMENT_CONNECTOR_TYPE='HIPAA' and DOCUMENT_UPLOAD_DATE >sysdate-365";
    public final static String UPDATE_DOCUMENT_STATUS =
  " UPDATE DFBS_DOCUMENTS_FIREHOUSE set DOCUMENT_STATUS='Approved' where DOCUMENT_ID=? ";
  public final static String SELECT_DOCUMENT_ID =
   "select DOCUMENT_ID.NEXTVAL from dual";
  public final static String SELECT_DOCUMENT_LIST_ADMIN =
  " SELECT DOCUMENT_NAME , DOCUMENT_TYPE, DOCUMENT_UPLOAD_DATE,DOCUMENT_ID ,DOCUMENT_UPLOADED_BY,DOCUMENT_STATUS,DOCUMENT_CONNECTOR " +
  " from DFBS_DOCUMENTS_FIREHOUSE WHERE  DOCUMENT_CONNECTOR_TYPE='HIPAA'   ";
}
